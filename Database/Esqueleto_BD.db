BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "CONTA" (
	"ID_CONTA"	INTEGER,
	"NUMERO_AGENCIA"	INTEGER,
	"NUMERO_CONTA"	INTEGER,
	"DIGITO_CONTA"	INTEGER,
	PRIMARY KEY("ID_CONTA")
);
CREATE TABLE IF NOT EXISTS "CLIENTE" (
	"EMAIL"	TEXT,
	"SENHA"	TEXT,
	"NOME"	TEXT,
	PRIMARY KEY("EMAIL")
);
CREATE TABLE IF NOT EXISTS "TRANSACAO" (
	"ID_TRANSACAO"	INTEGER,
	"DESCRICAO"	TEXT,
	"VALOR"	NUMERIC,
	"DATA"	NUMERIC,
	"TIPO_TRANSACAO"	TEXT,
	"FK_ID_CONTA_TRANSACAO_ CONTA"	INTEGER,
	"FK_EMAIL_TRANSACAO_CLIENTE"	TEXT,
	"FK_ID_TIPO_TRANSACAO_TRANSACAO_TIPOTRANSACAO"	INTEGER,
	FOREIGN KEY("FK_ID_TIPO_TRANSACAO_TRANSACAO_TIPOTRANSACAO") REFERENCES "TIPOTRANSACAO"("ID_TIPO_TRANSACAO"),
	FOREIGN KEY("FK_ID_CONTA_TRANSACAO_ CONTA") REFERENCES "CONTA"("ID_CONTA"),
	FOREIGN KEY("FK_EMAIL_TRANSACAO_CLIENTE") REFERENCES "CLIENTE"("EMAIL"),
	PRIMARY KEY("ID_TRANSACAO")
);
COMMIT;
